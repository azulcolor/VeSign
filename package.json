{
  "name": "vesignbackend",
  "version": "1.0.0",
  "description": "The BackEnd for the project 'veSing'. This is gonna contain a CRUD, JWT authentification and a script to sign pdf documents",
  "main": "./server/app.js",
  "type": "module",
  "scripts": {
    "test": "set NODE_OPTIONS=--experimental-vm-modules && jest --verbose --silent",
    "build": "next build ./view",
    "start": "nodemon src/app.js server/app.js",
    "start-front": "next start -p 3001 ./view",
    "dev": "next -p 3001 ./view",
    "post-update": "yarn upgrade --latest"
  },
